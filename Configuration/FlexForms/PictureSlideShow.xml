<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<langChildren>1</langChildren>
	</meta>
	<ROOT>
		<type>array</type>
		<el>
			<pictureSlideShowWidth>
				<TCEforms>
					<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:width</label>
					<config>
						<type>input</type>
						<size>8</size>
						<max>5</max>
						<eval>int</eval>
						<default>350</default>
					</config>
				</TCEforms>
			</pictureSlideShowWidth>
			<pictureSlideShowHeight>
				<TCEforms>
					<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:height</label>
					<config>
						<type>input</type>
						<size>8</size>
						<max>5</max>
						<eval>int</eval>
						<default>350</default>
					</config>
				</TCEforms>
			</pictureSlideShowHeight>
			<pictureSlideShowInterval>
				<TCEforms>
					<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:interval</label>
					<config>
						<type>input</type>
						<size>8</size>
						<max>10</max>
						<eval>int</eval>
						<default>30</default>
					</config>
				</TCEforms>
			</pictureSlideShowInterval>
			<pictureSlideShowContentSection>
				<tx_templavoila>
					<title>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:contents</title>
				</tx_templavoila>
				<type>array</type>
				<section>1</section>
				<el>
					<pictureSlideShowContent>
					<type>array</type>
					<tx_templavoila>
						<title>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:content</title>
					</tx_templavoila>
					<el>
						<picture>     
							<TCEforms type="array">
								<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:picture</label>
								<config type="array">
									<type>group</type>
									<internal_type>file</internal_type>
									<allowed>gif,png,jpg,jpeg</allowed>
									<max_size>1000</max_size>
									<uploadfolder>uploads/pics</uploadfolder>
									<show_thumbs>1</show_thumbs>
									<size>1</size>
									<maxitems>1</maxitems>
									<minitems>0</minitems>
								</config>
								<label>Image</label>
							</TCEforms>
						</picture>
						<caption>     
							<TCEforms type="array">
								<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:caption</label>
								<config type="array">
									<type>input</type>
									<size>30</size>
									<default></default>
								</config>
								<label>Image</label>
							</TCEforms>
						</caption>
						
					</el>	
					</pictureSlideShowContent>
				</el>
			</pictureSlideShowContentSection>
		</el>
	</ROOT>
</T3DataStructure>