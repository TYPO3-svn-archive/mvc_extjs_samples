<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<langChildren>1</langChildren>
	</meta>
	<ROOT>
		<type>array</type>
		<el>
			<settings.pictureSlideShowWidth>
				<TCEforms>
					<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:width</label>
					<config>
						<type>input</type>
						<size>8</size>
						<max>5</max>
						<eval>int</eval>
						<default>400</default>
					</config>
				</TCEforms>
			</settings.pictureSlideShowWidth>
			<settings.pictureSlideShowHeight>
				<TCEforms>
					<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:height</label>
					<config>
						<type>input</type>
						<size>8</size>
						<max>5</max>
						<eval>int</eval>
						<default>300</default>
					</config>
				</TCEforms>
			</settings.pictureSlideShowHeight>
			<settings.pictureSlideShowInterval>
				<TCEforms>
					<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:interval</label>
					<config>
						<type>input</type>
						<size>8</size>
						<max>10</max>
						<eval>int</eval>
						<default>3</default>
					</config>
				</TCEforms>
			</settings.pictureSlideShowInterval>
			<settings.pictureSlideShowShowPlayButton>
				<TCEforms>
					<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:ShowPlayButton</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</settings.pictureSlideShowShowPlayButton>
			<settings.pictureSlideShowAutoplay>
				<TCEforms>
					<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:autoplay</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</settings.pictureSlideShowAutoplay>
			<settings.pictureSlideShowPauseOnNavigate>
				<TCEforms>
					<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:PauseOnNavigate</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</settings.pictureSlideShowPauseOnNavigate>
			<settings.pictureSlideShowFreezeOnHoover>
				<TCEforms>
					<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:FreezeOnHoover</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</settings.pictureSlideShowFreezeOnHoover>
			<settings.pictureSlideShowNavigationOnHoover>
				<TCEforms>
					<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:NavigationOnHoover</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</settings.pictureSlideShowNavigationOnHoover>
			<settings.pictureSlideShowTransitionType>
				<TCEforms>
					<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:TransitionType</label>
					<config>
						<type>select</type>
						<items>
							<numIndex index="0">
								<numIndex index="0">LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:TransitionType.carousel</numIndex>
								<numIndex index="1">carousel</numIndex>
							</numIndex> 
							<numIndex index="1">
								<numIndex index="0">LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:TransitionType.fade</numIndex>
								<numIndex index="1">fade</numIndex>
							</numIndex> 
						</items>
					</config>
				</TCEforms>
			</settings.pictureSlideShowTransitionType>
			
			<settings.pictureSlideShowContentSection>
				<tx_templavoila>
					<title>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:contents</title>
				</tx_templavoila>
				<type>array</type>
				<section>1</section>
				<el>
					<pictureSlideShowContent>
						<type>array</type>
						<tx_templavoila>
							<title>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:content</title>
						</tx_templavoila>
						<el>
							<picture>     
								<TCEforms type="array">
									<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:picture</label>
									<config type="array">
										<type>group</type>
										<internal_type>file</internal_type>
										<allowed>gif,png,jpg,jpeg</allowed>
										<max_size>1000</max_size>
										<uploadfolder>uploads/pics</uploadfolder>
										<show_thumbs>1</show_thumbs>
										<size>1</size>
										<maxitems>1</maxitems>
										<minitems>0</minitems>
									</config>
									<label>Image</label>
								</TCEforms>
							</picture>
							<caption>     
								<TCEforms type="array">
									<label>LLL:EXT:mvc_extjs_samples/Resources/Private/Language/flexform.PictureSlideShow.xml:caption</label>
									<config type="array">
										<type>input</type>
										<size>30</size>
										<default></default>
									</config>
									<label>Image</label>
								</TCEforms>
							</caption>
							
						</el>	
					</pictureSlideShowContent>
				</el>
			</settings.pictureSlideShowContentSection>
		</el>
	</ROOT>
</T3DataStructure>