<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<langChildren>1</langChildren>
	</meta>
	<ROOT>
		<type>array</type>
		<el>
			<Twitter>
				<type>array</type>
				<tx_templavoila>
					<title>Twitter</title>
				</tx_templavoila>
				<el>
					<twitterWidth>
						<TCEforms>
							<label>Width:</label>
							<config>
								<type>input</type>
								<size>8</size>
								<max>5</max>
								<eval>int</eval>
								<default>350</default>
							</config>
						</TCEforms>
					</twitterWidth>
					<twitterHeight>
						<TCEforms>
							<label>Height:</label>
							<config>
								<type>input</type>
								<size>8</size>
								<max>5</max>
								<eval>int</eval>
								<default>350</default>
							</config>
						</TCEforms>
					</twitterHeight>
					<twitterImageWidth>
						<TCEforms>
							<label>Image width:</label>
							<config>
								<type>input</type>
								<size>8</size>
								<max>5</max>
								<eval>int</eval>
								<default>70</default>
							</config>
						</TCEforms>
					</twitterImageWidth>
					<twitterInterval>
						<TCEforms>
							<label>Refresh Interval (seconds):</label>
							<config>
								<type>input</type>
								<size>8</size>
								<max>10</max>
								<eval>int</eval>
								<default>30</default>
							</config>
						</TCEforms>
					</twitterInterval>
					<twitterType>
						<TCEforms>
							<label>Type:</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">User</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex> 
									<numIndex index="1">
										<numIndex index="0">Search</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex> 
								</items>
							</config>
						</TCEforms>
					</twitterType>
					<twitterKeyword>
						<TCEforms>
							<label>Keyword:</label>
							<config>
								<type>input</type>
								<size>30</size>
								<max>50</max>
								<default></default>
								<required>1</required>
							</config>
						</TCEforms>
					</twitterKeyword>
				</el>
			</Twitter>
		</el>
	</ROOT>
</T3DataStructure>